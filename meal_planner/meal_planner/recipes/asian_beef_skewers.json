    "steps" : [
        {
            "step_number" : 1,
            "step_description" : "Preheat a grill. If you are using wooden skewers, soak in water 30 minutes."
        },
        {
            "step_number" : 2,
            "step_description" : "In a gallon size sealable plastic bag, combine all of the ingredients, including the beef. Seal the bag, squeezing out the air. Marinate 15 minutes."
        },
        {
            "step_number" : 3,
            "step_description" : "With tongs, remove the beef from the marinade; discard the remaining marinade. On each of 16, six-inch, metal or wooden skewers, thread one beef strip, piercing the beef in several places."
        },
        {
            "step_number" : 4,
            "step_description" : "Grill until cooked through, about 5 minutes on each side."
        },
        {
            "step_number" : 5,
            "step_description" : "Serve with either chinese mustard, duck sauce and/ or soy sauce to dip the skewers inches."
        }
